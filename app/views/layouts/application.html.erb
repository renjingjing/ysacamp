<!DOCTYPE html>
<html>
<head>
  <title>StudentsInfo</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>

</head>
<body>
  <div class="container window-body">
    <div class="container">
      <%= render "/layouts/nav" %>

      <% if flash[:notice] %>
        <div class="alert alert-success" role="alert"><%= flash[:notice] %><i style="float:right" class="notice">x</i></div>
      <% elsif flash[:alert] %>
        <div class="alert alert-danger" role="alert"><%= flash[:alert] %></div>
      <% end %>
      <%= yield %>
    </div>
    <div class="container">
      <%= render "/layouts/footer" %>
    </div>
  </div>
</body>
</html>
