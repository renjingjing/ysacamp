<p id="notice"><%= notice %></p>
<p>
  <%# courses=@course_unit.course.all %>
  <% if @course_unit.course!= nil %>
      <%# courses.each do |course| %>
      <h4>Course: <%= link_to @course_unit.course.name,courses_path(search: @course_unit.course.name, order: "id") %></nobr></h4>
      <%# end %>
  <% end %>
</p>

<h4>
  <strong>Title:</strong>
  <%= @course_unit.title %>
</h4>

<p>
  <strong>Description:</strong>
  <%= @course_unit.description %>
</p>

<p>
  <strong>Note:</strong>
  <%= @course_unit.note %>
</p>

<% if can? :admin,current_user %>
  <%= link_to 'Edit', edit_course_unit_path(@course_unit) %> |
  <%= link_to 'Back', course_units_path %>
<% end %>
