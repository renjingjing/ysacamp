<div class="container contact-form">
  <div class="well well-lg">
    <h3>Contact Us</h3>
    <%= form_for @contact, url: contacts_path do |f|%>
      <div class="form-group">
        <%= f.label :message %>
        <%= f.text_field :message, class: "form-control" %>
      </div>
      <%= f.submit "Contact Us",class:"btn btn-primary" %>
    <% end %>
  </div>
</div>
