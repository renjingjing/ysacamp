/* HOVER STYLES */
div#pop-up {
  display: none;
  position: absolute;
  margin-left: -400px;
  margin-top: -20px;
  width: 600px;
  // height:100%;
  // padding: 5px;
  background: #f5f5f5;
  color: #000000;
  border: 1px solid #eeeeee;
  font-size: 70%;
}

#logo {
  -webkit-transition: -webkit-transform 2s; /* Safari */
  transition: transform 2s;
}

#logo:hover {
  -webkit-transform: rotate(180deg); /* Safari */
  transform: rotate(180deg);
}

