.carousel-control:hover,
.carousel-control:focus {
  color: #000;
  text-decoration: none;
  filter: alpha(opacity=100);
  outline: 0;
  opacity:0;
  background-color: white;
}

 .carousel-control.left {
   background-image: -webkit-linear-gradient(left, rgba(0, 0, 0, .5) 0%, rgba(0, 0, 0, .0001) 100%);
   background-image:      -o-linear-gradient(left, rgba(0, 0, 0, .5) 0%, rgba(0, 0, 0, .0001) 100%);
   background-image: -webkit-gradient(linear, left top, right top, from(rgba(0, 0, 0, .5)), to(rgba(0, 0, 0, .0001)));
   background-image:         linear-gradient(to right, rgba(0, 0, 0, .5) 0%, rgba(0, 0, 0, .0001) 100%);
   filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#80000000', endColorstr='#00000000', GradientType=1);
   background-repeat: repeat-x;
 }
 .carousel-control.right {
   right: 0;
   left: auto;
   background-image: -webkit-linear-gradient(left, rgba(0, 0, 0, .0001) 0%, rgba(0, 0, 0, .5) 100%);
   background-image:      -o-linear-gradient(left, rgba(0, 0, 0, .0001) 0%, rgba(0, 0, 0, .5) 100%);
   background-image: -webkit-gradient(linear, left top, right top, from(rgba(0, 0, 0, .0001)), to(rgba(0, 0, 0, .5)));
   background-image:         linear-gradient(to right, rgba(0, 0, 0, .0001) 0%, rgba(0, 0, 0, .5) 100%);
   filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000', endColorstr='#80000000', GradientType=1);
   background-repeat: repeat-x;
 }

.guide-container {
  background-color: #EFEFEF;
  padding: 20px;
  border-radius: 5px;
  box-shadow: 2px 2px 2px 2px rgba(204, 204, 204, 0.8);
  background-color:rgba(255, 255, 255, 0.6);
}

.golden-div {
  text-shadow: 1px 1px 2px gray;
  margin-top: 20px;
  padding-top: 30px;
  padding-bottom: 10px;
  padding-left: 10px;
  padding-right: 10px;
  margin-bottom: 30px;
  border-radius: 2px;
  box-shadow: 3px 3px 3px rgba(128, 113, 110, 1);
  min-width:36px;
  min-height:320px;
  // background-color:rgba(255, 204, 79, 0.6);
  background: -webkit-radial-gradient(rgba(255, 204, 79, 0.1), rgba(255, 204, 79, 1)); /* For Safari 5.1 to 6.0 */
  background: -o-radial-gradient(rgba(255, 204, 79, 0.1), rgba(255, 204, 79, 1)); /* For Opera 11.1 to 12.0 */
  background: -moz-radial-gradient(rgba(255, 204, 79, 0.1), rgba(255, 204, 79, 1)); /* For Firefox 3.6 to 15 */
  background: radial-gradient(rgba(255, 204, 79, 0.1), rgba(255, 204, 79, 1)); /* Standard syntax */
}

.golden-div-hover {
  // background-color: #FFCC4F;
  // text-shadow: 1px 1px 2px gray;
  margin-top: 20px;
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 10px;
  padding-right: 10px;
  margin-bottom: 30px;
  border-radius: 2px;
  min-width:36px;
  min-height:320px;
  background: -webkit-radial-gradient(rgba(239,180,3, 1), rgba(94, 187, 228, 0.8)); /* For Safari 5.1 to 6.0 */
  background: -o-radial-gradient(rgba(239,180,3, 1), rgba(94, 187, 228, 0.8)); /* For Opera 11.1 to 12.0 */
  background: -moz-radial-gradient(rgba(239,180,3, 1), rgba(94, 187, 228, 0.8)); /* For Firefox 3.6 to 15 */
  background: radial-gradient(rgba(239,180,3, 1), rgba(94, 187, 228, 0.8)); /* Standard syntax */
  // background: rgba(94, 187, 228, 1);
  opacity: 0.6;
  display:none;
  -webkit-transition: 1s opacity;
  -o-transition: 1s opacity;
  transition: 1s opacity;
  box-shadow: 3px 3px 3px rgba(128, 113, 110, 1);
  text-align: left;
  font-size: 16px;
  // font-weight: bold;
  color:black;
  text-shadow: 3px 3px 3px white;
}

